{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "cronos",
  "title": "Cronos",
  "description": "Manage scheduled commands from the Cronos menu bar app",
  "icon": "extension-icon.png",
  "author": "anonymous",
  "categories": ["Developer Tools", "Productivity"],
  "license": "MIT",
  "commands": [
    {
      "name": "list-jobs",
      "title": "List Jobs",
      "description": "View and manage scheduled Cronos jobs",
      "mode": "view"
    },
    {
      "name": "create-job",
      "title": "Create Job",
      "description": "Create a new scheduled job",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.87.1",
    "@raycast/utils": "^1.17.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "22.10.2",
    "@types/react": "18.3.17",
    "eslint": "^8.57.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
